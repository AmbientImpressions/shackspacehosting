<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<groupId>com.shackspacehosting</groupId>
	<artifactId>system</artifactId>
	<packaging>pom</packaging>
	<version>0.0.1-SNAPSHOT</version>
	<name>ShackSpace Hosting System Bootstrapper</name>

	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
		<project.rootDir>${project.basedir}/</project.rootDir>
		<build_timestamp>${maven.build.timestamp}</build_timestamp>
		<maven.build.timestamp.format>yyyy-MM-dd HH:mm</maven.build.timestamp.format>
		<vagrant.provider>virtualbox</vagrant.provider>
	</properties>

	<scm>
		<developerConnection>scm:git:https://github.com/dwimsey/shackspacehosting.git</developerConnection>
		<url>https://github.com/dwimsey/shackspacehosting</url>
	</scm>

</project>
